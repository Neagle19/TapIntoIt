<%= simple_form_for @user, url: registration_path(@user), html: { method: :put } do |f| %>
  <%= f.input :username %>
  <%= f.input :password %>
  <%= f.input :first_name%>
  <%= f.input :last_name%>
  <%= f.input :email%>
  <%= f.input :description%>
  <%= f.input :location%>
  <%= f.input :photo%>
  <%= f.button :submit %>
<% end %>
